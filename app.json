{
  "expo": {
    "name": "GradeTree",
    "description": "Check your grade and attendance with GradeTree.",
    "slug": "GradeTree",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.5.1",
    "orientation": "portrait",
    "icon": "./assets/images/AspenMobile.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kevingaojx.cpsgradetree"
    },
    "android": {
      "package": "com.kevingaojx.gradetree",
      "permissions": [],
      "versionCode": 3
    }
  }
}
